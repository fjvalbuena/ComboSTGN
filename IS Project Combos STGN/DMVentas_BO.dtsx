          SQLTask:SqlStatementSource="update JerCliente set&#xA;JerCliente.NombreNodoJerarquia= zDmJerCl.NombreNodoJerarquia&#xA;FROM lukerVen.SD.JerCliente JerCliente, HBAVENNAL.STGN.zDmJerCl zDmJerCl&#xA;WHERE zDmJerCl.Cod_Canal = JerCliente.Cod_Canal &#xA;AND zDmJerCl.JerCliente = JerCliente.JerCliente &#xA;AND ((JerCliente.NombreNodoJerarquia&lt;&gt;zDmJerCl.NombreNodoJerarquia))&#xA;&#xA;&#xA;&#xA;update Clientes set Clientes.Gerente_Comercial= JerCliente.NombreNodoJerarquia&#xA;FROM lukerVen.SD.Clientes Clientes, lukerVen.SD.JerCliente JerCliente&#xA;WHERE JerCliente.Cod_Canal = Clientes.Cod_Canal &#xA;AND Clientes.JerN1 = JerCliente.JerCliente &#xA;AND ((Clientes.Gerente_Comercial&lt;&gt;JerCliente.NombreNodoJerarquia))&#xA;&#xA;update Clientes set  Clientes.Director_Canal= JerCliente.NombreNodoJerarquia&#xA;FROM lukerVen.SD.Clientes Clientes, lukerVen.SD.JerCliente JerCliente&#xA;WHERE JerCliente.Cod_Canal = Clientes.Cod_Canal &#xA;AND Clientes.JerN2 = JerCliente.JerCliente &#xA;AND ((Clientes.Director_Canal&lt;&gt;JerCliente.NombreNodoJerarquia))&#xA;&#xA;update Clientes set   Clientes.Lider_Regional= JerCliente.NombreNodoJerarquia&#xA;FROM lukerVen.SD.Clientes Clientes, lukerVen.SD.JerCliente JerCliente&#xA;WHERE JerCliente.Cod_Canal = Clientes.Cod_Canal AND Clientes.JerN3Origen = JerCliente.JerCliente AND ((Clientes.Lider_Regional&lt;&gt;JerCliente.NombreNodoJerarquia))&#xA;&#xA;update Clientes set Clientes.Poblacion= zDmCli.Poblacion&#xA;FROM lukerVen.SD.Clientes Clientes, HBAVENNAL.STGN.zDmCli zDmCli&#xA;WHERE zDmCli.Cod_Cliente = Clientes.Cod_Cliente &#xA;and Clientes.Poblacion&lt;&gt; zDmCli.Poblacion&#xA;&#xA;update Clientes set &#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#xA;  Clientes.Nombre_Cliente&#x9;=&#x9; zDmCli.Nombre_Cliente&#x9;&#x9;&#xA;, Clientes.Cod_Canal&#x9;=&#x9; isnull(zDmCli.Cod_Canal,'')&#x9;&#x9;&#xA;, Clientes.Nombre_Canal&#x9;=&#x9; isnull(zDmCli.Nombre_Canal,'')&#x9;&#x9;&#xA;, Clientes.Cod_Pais&#x9;=&#x9; zDmCli.Cod_Pais&#x9;&#x9;&#xA;, Cl