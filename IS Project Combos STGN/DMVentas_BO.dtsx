          SQLTask:SqlStatementSource="USE [lukerVen]&#xA;GO&#xA;&#xA;&#xA;insert into lukerVen.SD.ClientesDimCT&#xA;SELECT     Clientes.Cod_Cliente, Clientes.Nombre_Cliente, CAST(CASE isnull(EquivalenciaGrupoClientes.CodCanal_E, 'Nu') &#xA;                      WHEN 'Nu' THEN Clientes.Cod_Canal ELSE isnull(EquivalenciaGrupoClientes.CodCanal_E, 'Nu') END AS varchar(2)) AS 'Cod_Canal', &#xA;                      CAST(CASE isnull(EquivalenciaGrupoClientes.NombreCanal_E, 'Nulo') &#xA;                      WHEN 'Nulo' THEN Clientes.Nombre_Canal ELSE isnull(EquivalenciaGrupoClientes.NombreCanal_E, 'Nulo') END AS varchar(35)) AS 'Nombre_Canal', &#xA;                      Clientes.Cod_Pais, CAST(CASE isnull(EquivalenciaGrupoClientes.CodGCliente_E, 'Nu') &#xA;                      WHEN 'Nu' THEN Clientes.Cod_GCliente ELSE isnull(EquivalenciaGrupoClientes.CodGCliente_E, 'Nu') END AS varchar(2)) AS 'Cod_GCliente', &#xA;                      CAST(CASE isnull(EquivalenciaGrupoClientes.NombreGCliente_E, 'Nulo') &#xA;                      WHEN 'Nulo' THEN Clientes.Nombre_GCliente ELSE isnull(EquivalenciaGrupoClientes.NombreGCliente_E, 'Nulo') END AS varchar(20)) AS 'Nombre_GCliente', &#xA;                      Clientes.Cod_Oficina_Ventas, Clientes.Nombre_Oficina_Ventas, Clientes.Cod_Zona_Ventas, Clientes.Cod_Vendedor, Clientes.Nombre_Vendedor, &#xA;                      Clientes.Cod_Region, Clientes.Nombre_Ciudad, Clientes.Cod_Central, Clientes.Cod_GCliente_AD1, Clientes.Nombre_GCliente_AD1, Clientes.Cod_GCliente_AD2, &#xA;                      Clientes.Nombre_GCliente_AD2, Clientes.Cod_Pagador, Clientes.Telebox, Clientes.Gerente_Comercial, Clientes.Director_Canal, Clientes.Lider_Regional, &#xA;                      Clientes.Jefe_Ventas, Clientes.Fecha_Creacion_Cliente, Clientes.Estatus, Clientes.Cod_Ruta, Clientes.CoordenadasX, Clientes.CoordenadasY&#xA;FROM         SD.ClientesJerQuv AS Clientes LEFT OUTER JOIN&#xA;                      SD.EquivalenciaGrupoClientesNombres AS EquivalenciaGrupoClientes ON Clientes.Cod_GCliente = EquivalenciaGrupoClientes.Cod_GCliente AND &#xA;                      Clientes.Cod_Canal = EquivalenciaGrupoClientes.Cod_Canal&#xA;where Clientes.Cod_Cliente not in (&#xA;SELECT ClientesDimCT.Cod_Cliente&#xA;FROM lukerVen.SD.ClientesDimCT ClientesDimCT&#xA;)&#xA;&#xA;&#xA;insert into [SRVDBLUKBO01\B2C].[lukerVen].[SD].[ClientesDimCT]&#xA;           ([Cod_Cliente]&#xA;           ,[Nombre_Cliente]&#xA;           ,[Cod_Canal]&#xA;           ,[Nombre_Canal]&#xA;           ,[Cod_Pais]&#xA;           ,[Cod_GCliente]&#xA;           ,[Nombre_GCliente]&#xA;           ,[Cod_Oficina_Ventas]&#xA;           ,[Nombre_Oficina_Ventas]&#xA;           ,[Cod_Zona_Ventas]&#xA;           ,[Cod_Vendedor]&#xA;           ,[Nombre_Vendedor]&#xA;           ,[Cod_Region]&#xA;           ,[Nombre_Ciudad]&#xA;           ,[Cod_Central]&#xA;           ,[Cod_GCliente_AD1]&#xA;           ,[Nombre_GCliente_AD1]&#xA;           ,[Cod_GCliente_AD2]&#xA;           ,[Nombre_GCliente_AD2]&#xA;           ,[Cod_Pagador]&#xA;           ,[Telebox]&#xA;           ,[Gerente_Comercial]&#xA;           ,[Director_Canal]&#xA;           ,[Lider_Regional]&#xA;           ,[Jefe_Ventas]&#xA;           ,[Fecha_Creacion_Cliente]&#xA;           ,[Estatus]&#xA;           ,[Cod_Ruta]&#xA;           ,[CoordenadasX]&#xA;           ,[CoordenadasY])&#xA;&#xA;select            [Cod_Cliente]&#xA;           ,[Nombre_Cliente]&#xA;           ,[Cod_Canal]&#xA;           ,[Nombre_Canal]&#xA;           ,[Cod_Pais]&#xA;           ,[Cod_GCliente]&#xA;           ,[Nombre_GCliente]&#xA;           ,[Cod_Oficina_Ventas]&#xA;           ,[Nombre_Oficina_Ventas]&#xA;           ,[Cod_Zona_Ventas]&#xA;           ,[Cod_Vendedor]&#xA;           ,[Nombre_Vendedor]&#xA;           ,[Cod_Region]&#xA;           ,[Nombre_Ciudad]&#xA;           ,[Cod_Central]&#xA;           ,[Cod_GCliente_AD1]&#xA;           ,[Nombre_GCliente_AD1]&#xA;           ,[Cod_GCliente_AD2]&#xA;           ,[Nombre_GCliente_AD2]&#xA;           ,[Cod_Pagador]&#xA;           ,[Telebox]&#xA;           ,[Gerente_Comercial]&#xA;           ,[Director_Canal]&#xA;           ,[Lider_Regional]&#xA;           ,[Jefe_Ventas]&#xA;           ,[Fecha_Creacion_Cliente]&#xA;           ,[Estatus]&#xA;           ,[Cod_Ruta]&#xA;           ,[CoordenadasX]&#xA;           ,[CoordenadasY]&#xA; from [lukerVen].[SD].[ClientesDimCT]&#xA;where [Cod_Cliente] not in (&#xA;  select [Cod_Cliente] from [SRVDBLUKBO01\B2C].[lukerVen].[SD].[ClientesDimCT]&#xA;)&#xA;&#xA;update BO set&#xA;            BO.[Cod_Cliente]= CL.[Cod_Cliente]&#xA;           ,BO.[Nombre_Cliente]= CL.[Nombre_Cliente]&#xA;           ,BO.[Cod_Canal]= CL.[Cod_Canal]&#xA;           ,BO.[Nombre_Canal]= CL.[Nombre_Canal]&#xA;           ,BO.[Cod_Pais]= CL.[Cod_Pais]&#xA;           ,BO.[Cod_GCliente]= CL.[Cod_GCliente]&#xA;           ,BO.[Nombre_GCliente]= CL.[Nombre_GCliente]&#xA;           ,BO.[Cod_Oficina_Ventas]= CL.[Cod_Oficina_Ventas]&#xA;           ,BO.[Nombre_Oficina_Ventas]= CL.[Nombre_Oficina_Ventas]&#xA;           ,BO.[Cod_Zona_Ventas]= CL.[Cod_Zona_Ventas]&#xA;           ,BO.[Cod_Vendedor]= CL.[Cod_Vendedor]&#xA;           ,BO.[Nombre_Vendedor]= CL.[Nombre_Vendedor]&#xA;           ,BO.[Cod_Region]= CL.[Cod_Region]&#xA;           ,BO.[Nombre_Ciudad]= CL.[Nombre_Ciudad]&#xA;           ,BO.[Cod_Central]= CL.[Cod_Central]&#xA;           ,BO.[Cod_GCliente_AD1]= CL.[Cod_GCliente_AD1]&#xA;           ,BO.[Nombre_GCliente_AD1]= CL.[Nombre_GCliente_AD1]&#xA;           ,BO.[Cod_GCliente_AD2]= CL.[Cod_GCliente_AD2]&#xA;           ,BO.[Nombre_GCliente_AD2]= CL.[Nombre_GCliente_AD2]&#xA;           ,BO.[Cod_Pagador]= CL.[Cod_Pagador]&#xA;           ,BO.[Telebox]= CL.[Telebox]&#xA;           ,BO.[Gerente_Comercial]= CL.[Gerente_Comercial]&#xA;           ,BO.[Director_Canal]= CL.[Director_Canal]&#xA;           ,BO.[Lider_Regional]= CL.[Lider_Regional]&#xA;           ,BO.[Jefe_Ventas]= CL.[Jefe_Ventas]&#xA;           ,BO.[Fecha_Creacion_Cliente]= CL.[Fecha_Creacion_Cliente]&#xA;           ,BO.[Estatus]= CL.[Estatus]&#xA;           ,BO.[Cod_Ruta]= CL.[Cod_Ruta]&#xA;           ,BO.[CoordenadasX]= CL.[CoordenadasX]&#xA;           ,BO.[CoordenadasY]= CL.[CoordenadasY]&#xA;&#xA;&#x9;&#x9;   from [SRVDBLUKBO01\B2C].[lukerVen].[SD].[ClientesDimCT] BO inner join [SD].[ClientesDimCT] CL on  BO.[Cod_Cliente]= CL.[Cod_Cliente]&#xA;    where     BO.[Cod_Cliente]&lt;&gt; CL.[Cod_Cliente]&#xA;           or BO.[Nombre_Cliente]&lt;&gt; CL.[Nombre_Cliente]&#xA;           or BO.[Cod_Canal]&lt;&gt; CL.[Cod_Canal]&#xA;           or BO.[Nombre_Canal]&lt;&gt; CL.[Nombre_Canal]&#xA;           or BO.[Cod_Pais]&lt;&gt; CL.[Cod_Pais]&#xA;           or BO.[Cod_GCliente]&lt;&gt; CL.[Cod_GCliente]&#xA;           or BO.[Nombre_GCliente]&lt;&gt; CL.[Nombre_GCliente]&#xA;           or BO.[Cod_Oficina_Ventas]&lt;&gt; CL.[Cod_Oficina_Ventas]&#xA;           or BO.[Nombre_Oficina_Ventas]&lt;&gt; CL.[Nombre_Oficina_Ventas]&#xA;           or BO.[Cod_Zona_Ventas]&lt;&gt; CL.[Cod_Zona_Ventas]&#xA;           or BO.[Cod_Vendedor]&lt;&gt; CL.[Cod_Vendedor]&#xA;           or BO.[Nombre_Vendedor]&lt;&gt; CL.[Nombre_Vendedor]&#xA;           or BO.[Cod_Region]&lt;&gt; CL.[Cod_Region]&#xA;           or BO.[Nombre_Ciudad]&lt;&gt; CL.[Nombre_Ciudad]&#xA;           or BO.[Cod_Central]&lt;&gt; CL.[Cod_Central]&#xA;           or BO.[Cod_GCliente_AD1]&lt;&gt; CL.[Cod_GCliente_AD1]&#xA;           or BO.[Nombre_GCliente_AD1]&lt;&gt; CL.[Nombre_GCliente_AD1]&#xA;           or BO.[Cod_GCliente_AD2]&lt;&gt; CL.[Cod_GCliente_AD2]&#xA;           or BO.[Nombre_GCliente_AD2]&lt;&gt; CL.[Nombre_GCliente_AD2]&#xA;           or BO.[Cod_Pagador]&lt;&gt; CL.[Cod_Pagador]&#xA;           or BO.[Telebox]&lt;&gt; CL.[Telebox]&#xA;           or BO.[Gerente_Comercial]&lt;&gt; CL.[Gerente_Comercial]&#xA;           or BO.[Director_Canal]&lt;&gt; CL.[Director_Canal]&#xA;           or BO.[Lider_Regional]&lt;&gt; CL.[Lider_Regional]&#xA;           or BO.[Jefe_Ventas]&lt;&gt; CL.[Jefe_Ventas]&#xA;           or BO.[Fecha_Creacion_Cliente]&lt;&gt; CL.[Fecha_Creacion_Cliente]&#xA;           or BO.[Estatus]&lt;&gt; CL.[Estatus]&#xA;           or BO.[Cod_Ruta]&lt;&gt; CL.[Cod_Ruta]&#xA;           or BO.[CoordenadasX]&lt;&gt; CL.[CoordenadasX]&#xA;           or BO.[CoordenadasY]&lt;&gt; CL.[CoordenadasY]&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
