'' then '0' else isnull(&#x9; zDmCli.CoordenadasX&#x9;,'0')  end&#xA;, Clientes.CoordenadasY&#x9;=&#x9; case zDmCli.CoordenadasY when '' then '0' else isnull(&#x9; zDmCli.CoordenadasY&#x9;,'0')  end&#x9;&#x9;&#xA;, Clientes.Cedi&#x9;=&#x9; zDmCli.Cedi&#x9;&#x9;&#xA;, Clientes.Cod_CondiPago&#x9;=&#x9; zDmCli.Cod_CondiPago&#x9;&#x9;&#xA;, Clientes.Barrio&#x9;=&#x9; zDmCli.Barrio&#x9;&#x9;&#xA;, Clientes.Cod_Blck&#x9;=&#x9; zDmCli.Cod_Blck&#x9;&#x9;&#xA;&#x9;&#xA;, Clientes.Poblacion&#x9;=&#x9; zDmCli.Poblacion&#x9;&#x9;&#xA;FROM lukerVen.SD.Clientes Clientes, HBAVENNAL.STGN.zDmCli zDmCli&#x9;&#x9;&#x9;&#x9;&#xA;WHERE Clientes.Cod_Cliente = zDmCli.Cod_Cliente&#x9;&#x9;&#xA;&#xA;and (&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#xA; &#xA; Clientes.Nombre_Cliente&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Nombre_Cliente&#x9;,'')&#xA;or Clientes.Cod_Canal&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Canal&#x9;,'')&#xA;or Clientes.Nombre_Canal&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Nombre_Canal&#x9;,'')&#xA;or Clientes.Cod_Pais&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Pais&#x9;,'')&#xA;or Clientes.Cod_GCliente&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_GCliente&#x9;,'')&#xA;or Clientes.Nombre_GCliente&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Nombre_GCliente&#x9;,'')&#xA;or Clientes.Cod_Oficina_Ventas&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Oficina_Ventas&#x9;,'')&#xA;or Clientes.Nombre_Oficina_Ventas&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Nombre_Oficina_Ventas&#x9;,'')&#xA;or Clientes.Cod_Zona_Ventas&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Zona_Ventas&#x9;,'')&#xA;or Clientes.Cod_Vendedor&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Vendedor&#x9;,'')&#xA;or Clientes.Nombre_Vendedor&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Nombre_Vendedor&#x9;,'')&#xA;or Clientes.Cod_Region&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Region&#x9;,'')&#xA;or Clientes.Nombre_Ciudad&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Nombre_Ciudad&#x9;,'')&#xA;or Clientes.Cod_Central&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Central&#x9;,'')&#xA;or Clientes.Cod_GCliente_AD1&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_GCliente_AD1&#x9;,'')&#xA;or Clientes.Nombre_GCliente_AD1&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Nombre_GCliente_AD1&#x9;,'')&#xA;or Clientes.Cod_GCliente_AD2&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_GCliente_AD2&#x9;,'')&#xA;or Clientes.Nombre_GCliente_AD2&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Nombre_GCliente_AD2&#x9;,'')&#xA;or Clientes.Cod_Pagador&#x9;&lt;&gt;&#x9;isnull(zDmCli.Cod_Pagador, zDmCli.Cod_Cliente)&#xA;or Clientes.Telebox&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Telebox&#x9;,'')&#xA;or Clientes.Gerente_Comercial&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Gerente_Comercial&#x9;,'')&#xA;or Clientes.Director_Canal&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Director_Canal&#x9;,'')&#xA;or Clientes.Lider_Regional&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Lider_Regional&#x9;,'')&#xA;or Clientes.Jefe_Ventas&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Jefe_Ventas&#x9;,'')&#xA;or Clientes.Fecha_Creacion_Cliente&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Fecha_Creacion_Cliente&#x9;,'')&#xA;or Clientes.Estatus&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.esActivo&#x9;,'')&#xA;&#xA;or Clientes.Cod_Ruta&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Ruta&#x9;,'')&#xA;or Clientes.CoordenadasX&#x9;&lt;&gt;&#x9;case zDmCli.CoordenadasX when '' then '0' else isnull(&#x9; zDmCli.CoordenadasX&#x9;,'0')  end&#xA;or Clientes.CoordenadasY&#x9;&lt;&gt;&#x9;case zDmCli.CoordenadasY when '' then '0' else isnull(&#x9; zDmCli.CoordenadasY&#x9;,'0')  end&#xA;or Clientes.Cedi&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cedi&#x9;,'')&#xA;or Clientes.Cod_CondiPago&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_CondiPago&#x9;,'')&#xA;or Clientes.Barrio&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Barrio&#x9;,'')&#xA;or Clientes.Cod_Blck&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Cod_Blck&#x9;,'')&#xA;&#xA;or Clientes.Poblacion&#x9;&lt;&gt;&#x9;isnull(&#x9; zDmCli.Poblacion&#x9;,'')&#xA;)&#x9;&#xA;&#xA;&#xA;/**&#xA;&#x9;&#x9;&#x9;AMIGOS DE LA CASA Y VENTAS POS&#xA;**/&#xA;update JerC